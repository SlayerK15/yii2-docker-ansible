# Fixed hosts file format
[all]
yii2-manager ansible_host=ip ansible_user=ubuntu ansible_ssh_private_key_file=./key.pem
yii2-worker1 ansible_host=ip ansible_user=ubuntu ansible_ssh_private_key_file=./key.pem
yii2-worker2 ansible_host=ip ansible_user=ubuntu ansible_ssh_private_key_file=./key.pem

[all:vars]
ansible_ssh_common_args='-o StrictHostKeyChecking=no'
ansible_python_interpreter=/usr/bin/python3